{
    "swagger": "2.0",
    "info": {
        "version": "1.0.0",
        "title": "Nudity Detection",
        "description": "Detects extent of nudity in pictures."
    },
    "schemes": [
        "https"
    ],
    "host": "simple.api",
    "basePath": "https://api.algorithmia.com/v1/algo/sfw/NudityDetection/1.1.4",
    "paths": {
        "/persons": {
            "get": {
                "summary": "Evaluate a picture for nudity",
                "description": "Returns a binary judgement and confidence level whether nudity is in the picture.",
                "responses": {
                    "200": {
                        "description": "A list of Person",
                        "schema": {
                            "type": "array",
                            "items": {
                                "properties": {
                                    "firstName": {
                                        "type": "string"
                                    },
                                    "lastName": {
                                        "type": "string"
                                    },
                                    "username": {
                                        "type": "string"
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
    }
}
